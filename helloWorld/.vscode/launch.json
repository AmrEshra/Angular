{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch helloWorld.ts",
      "type": "node",
      "console": "integratedTerminal",
      "request": "launch",
      "program": "${workspaceFolder}/src/app/helloWorld.ts",
      "cwd": "${workspaceFolder}/src/app"
    },
    {
      "name": "Launch Chrome helloWorld on Angular CLI Server",
      "type": "chrome",
      "request": "launch",
      "port": 9225,
      "webRoot": "${workspaceFolder}",
      "url": "http://localhost:4200/#"
    }
  ]
}